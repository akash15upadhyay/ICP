class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        int idx = nums1.length-n;
        int k=0;
        for(int i=idx;i<nums1.length;i++){
            int temp = nums1[i];
            nums1[i] = nums2[k];
            nums2[k] = temp;
            k++;
        }
        Arrays.sort(nums1);
    }
}
