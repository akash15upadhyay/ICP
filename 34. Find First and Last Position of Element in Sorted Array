class Solution {
    public int[] searchRange(int[] arr, int target) {
        int ans[]= new int[2];
        int minidx=-1;
        int maxidx=-1;
        int start=0;
        int end =arr.length-1;
        if(arr.length==0){
            ans[0]=ans[1]=-1;
            return ans;
        }
        while(start<=end){
            int mid = start + (end-start)/2;
            if(arr[mid]==target){
                minidx=mid;
                end =mid-1;
            }
            else if(arr[mid]>target ){
                end = mid-1;
            }
            else if(arr[mid]<target){
                start = mid+1;
            }
        }
        start=0;
        end =arr.length-1;
        while(start<=end){
            int mid = start + (end-start)/2;
            if(arr[mid]==target){
                maxidx=mid;
                start = mid+1;
            }
            else if(arr[mid]>target){
                end = mid-1;
            }
            else if(arr[mid]<target ){
                start = mid+1;
            }
        }
        ans[0] =minidx;
        ans[1] = maxidx;
        return ans;  
    }
}
